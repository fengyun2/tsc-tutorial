{
    "root": true,
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "ecmaFeatures": {},
    "globals": {

    },
    "plugins": [
        "html"
    ],
    "extends": "eslint-config-airbnb",
    "rules": {
        "comma-dangle": 0,
        "no-console": 0,
        "id-length": 0,
        "semi": 0,
        "quotes": [
            2
        ],
        "arrow-parens": 0,
        "generator-star-spacing": 0
    },
    "settings": {}
}
